space Vectra.Test;

class Player {
    number Age { get; set; }
    number Wins = 0;
    number Losses = 0;
    Player(number age) {
        Age = age;
    }
    
    void Win() {
        Wins = Wins + 1;
    }
    
    void Lose() {
        Losses = Losses + 1;
    }
    
    number GetVictoryPercentage() {
        let totalMatches = Wins + Losses;
        return Wins / totalMatches;
    }
}

class Observer {
    void Observe(Player player) {
    }
}